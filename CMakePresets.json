{
    "version": 3,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 19,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "debug",
        "displayName": "debug",
        "cacheVariables": {
          "CMAKE_CXX_FLAGS": "-Wall -Wextra -pedantic",
          "CMAKE_CXX_STANDARD": "17",
          "CMAKE_CXX_STANDARD_REQUIRED": "YES",
          "CMAKE_CXX_EXTENSIONS": "OFF"
        }
      }, 
      {
        "name": "fast",
        "displayName": "fast",
        "cacheVariables": {
          "CMAKE_CXX_FLAGS": "-O3",
          "CMAKE_CXX_STANDARD": "17",
          "CMAKE_CXX_STANDARD_REQUIRED": "YES",
          "CMAKE_CXX_EXTENSIONS": "OFF"
        }
      },
      {
        "name": "fastest",
        "displayName": "fastest",
        "cacheVariables": {
          "CMAKE_CXX_FLAGS": "-DNDEBUG -O3",
          "CMAKE_CXX_STANDARD": "17",
          "CMAKE_CXX_STANDARD_REQUIRED": "YES",
          "CMAKE_CXX_EXTENSIONS": "OFF"
        }
      },
      {
        "name": "default",
        "displayName": "default",
        "cacheVariables": {
          "CMAKE_CXX_FLAGS": " ",
          "CMAKE_CXX_STANDARD": "17",
